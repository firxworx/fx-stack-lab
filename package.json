  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rimraf dist",
    "start": "nx run-many --all --target=serve --parallel",
    "start:solo": "nx serve solo",
    "prebuild": "",
    "postbuild": "",
    "build": "nx run-many --all --target=build --parallel",
    "build:clean": "nx run-many --all --target=build --skip-nx-cache --verbose --output-style=static",
    "lint": "nx run-many --target=lint --projects=ui,api --parallel",
    "test": "nx run-many --all --target=test --verbose --output-style=static",
    "test:clean": "nx run-many --all --target=test --skip-nx-cache --verbose --output-style=static",
    "test:affected": "nx affected:test --all --codeCoverage --skip-nx-cache --verbose --output-style=static",
    "prettier": "prettier --config .prettierrc --write .",
    "docker:up": "docker compose up",
    "docker:down": "docker compose down",
    "nx:migrate:latest": "nx migrate @nrwl/workspace@latest && pnpm exec nx migrate --run-migrations --if-exists"
  },
